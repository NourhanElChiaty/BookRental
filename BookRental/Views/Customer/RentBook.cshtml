@model BookRental.Models.Rental
@{
    ViewBag.Title = "RentBook";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>RentBook?</h2>
@using (Html.BeginForm("RentBook", "Customer", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <button type="submit" class="btn btn-default col-lg-offset-2 submit">Submit</button>

}

@if (ViewBag.Message != null)
{
    <script>

$(document).ready(function(){

alert('@ViewBag.Message');

});

    </script>

}